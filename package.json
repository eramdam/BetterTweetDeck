    "@types/jquery": "^3.5.9",
    "@types/lodash": "^4.14.177",
    "@types/luxon": "^2.0.7",
    "@types/node": "^16.11.10",
    "@types/prettier": "^2.4.2",
    "@types/react": "^17.0.36",
