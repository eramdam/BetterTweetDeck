    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "execa": "^4.1.0",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
